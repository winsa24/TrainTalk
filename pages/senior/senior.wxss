/* pages/senior/senior.wxss */
button{
  position: absolute;
  top: 700px;
  left: 30px
}
div{
  position: absolute;
  background: #B9E3C6;
  width: 100px;
  height: 120px;
  top: 300px;
  left: 150px;
  animation-iteration-count: 2;   /* 动画播放两次 */
  animation-direction: alternate; /* 来回 */
}

@keyframes m1{
  0%{
    transform: translateX(0px);
  }
  100%{
    transform: translateX(200px);
  }
}
@keyframes m2{
  0%{
    transform: translateY(0px);
  }
  100%{
    transform: translateY(200px);
  }
}@keyframes m3{
  0%{
    transform: translateX(0px);
  }
  100%{
    transform: translateX(-200px);
  }
}@keyframes m4{
  0%{
    transform: translateY(0px);
  }
  100%{
    transform: translateY(-200px);
  }
}
@keyframes m11{
  0%{
    transform: translateX(0px) translateY(0px);
  }
  100%{
    transform: translateX(200px) translateY(200px) rotate(90deg);
  }
}
@keyframes m21{
  0%{
    transform: translateX(0px) translateY(0px);
  }
  100%{
    transform: translateX(200px) translateY(-200px) rotate(90deg);
  }
}@keyframes m31{
  0%{
    transform: translateX(0px) translateY(0px);
  }
  100%{
    transform: translateX(-200px) translateY(200px) rotate(90deg);
  }
}@keyframes m41{
  0%{
    transform: translateX(0px) translateY(0px);
  }
  100%{
    transform: translateX(-200px) translateY(-200px) rotate(90deg);
  }
}