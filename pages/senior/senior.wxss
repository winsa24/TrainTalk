  margin-top: 100rpx;
  background-color: #FFCC00;
  color: #FFF;
}
.start{
  position: absolute;
  top: 80px;
  left: 110px;
}
.pa{
  position: absolute;
  bottom: 100px;
  left: 110px;
}
.time{
  position: absolute;
  bottom: 200px;
  left: 160px;
}
.word_l {
  position: absolute;
  width: 80px;
  height: 120px;
  top: 330px;
  left: 50px;
  font-size: 48rpx;
  color: #FFF;
}
.word_m{
  position: absolute;
  width: 80px;
  height: 120px;
  top: 330px;
  left: 160px;
  font-size: 48rpx;
  color: #FFF;
